{
  "private": true,
  "engines": {
    "node": ">=23.11.1",
    "pnpm": ">=10.14.0"
  },
  "scripts": {
    "dev": "pnpm next dev",
    "start": "NODE_ENV=production pnpm next start",
    "start:dev": "cp .env.dev .env && pnpm dev",
    "start:prod": "cp .env.prod .env && pnpm dev",
    "build": "NODE_ENV=production pnpm next build",
    "prettier": "pnpm dlx prettier --write --ignore-unknown .",
    "prettier:check": "pnpm dlx prettier --check --ignore-unknown .",
    "test": "pnpm dlx pnpm prettier:check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@types/react-dom": "^19.1.7",
    "clsx": "^2.1.1",
    "geist": "^1.4.2",
    "next": "15.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "5.9.2"
  }
}
