#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/@tailcallhq+core-darwin-x64@1.6.14/node_modules/@tailcallhq/core-darwin-x64/node_modules:/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/@tailcallhq+core-darwin-x64@1.6.14/node_modules:/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/@tailcallhq+core-darwin-x64@1.6.14/node_modules/@tailcallhq/core-darwin-x64/node_modules:/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/@tailcallhq+core-darwin-x64@1.6.14/node_modules:/Users/user/myprojects/shortlink/shop/oms-graphql/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../../@tailcallhq+core-darwin-x64@1.6.14/node_modules/@tailcallhq/core-darwin-x64/bin/tailcall"   "$@"
exit $?
